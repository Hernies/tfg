{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "(gdb) Launch traversecsv",
            "type": "cppdbg",
            "request": "launch",
            "cwd": "${fileDirname}",
            "program": "${workspaceFolder}/datasets/traversecsv",
            "args" : ["CLEAN_REFIT_081116/CLEAN_House11.csv","12"],
            "MIMode": "gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                },
                {
                    "description": "Set Disassembly Flavor to Intel",
                    "text": "-gdb-set disassembly-flavor intel",
                    "ignoreFailures": true
                }
            ]
        },
        {
            "name": "(gdb) Launch host",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/encoding/host",
            "args": [1, 1, 1000, 2, 1],
            "stopAtEntry": false,
            "cwd": "${fileDirname}",
            "environment": [],
            "externalConsole": false,
            "MIMode": "gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                },
                {
                    "description": "Set Disassembly Flavor to Intel",
                    "text": "-gdb-set disassembly-flavor intel",
                    "ignoreFailures": true
                }
            ]
        },
    ]
}